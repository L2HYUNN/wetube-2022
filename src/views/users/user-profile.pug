extends ../base
include ../mixins/video

block content
    div.profile__data
    if loggedInUser.socialOnly
        img(src=`${loggedInUser.avatarUrl}`, alt="").avatar       
    else
        img(src=`/${loggedInUser.avatarUrl}`, alt="").avatar       
        h2 #{user.username} 
        h2 #{user.email}
    

    div.video-grid
        each info in videos
            +video(info)
        else 
            span.empty__message No videos found
